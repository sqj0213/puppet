
backend backend_img_old
    mode       http
    contimeout 5000
    srvtimeout 50000
    balance    roundrobin
    hash-type  consistent
    option http-pretend-keepalive
    option srvtcpka
    option httpchk HEAD /lost.gif HTTP/1.1\r\nHost:p1.gexing.com\r\n Referer:http://www.gexing.com
    server  172.16.2.21:8003  172.16.2.21:8003 id 03 cookie img_old03 check inter 2s downinter 5s slowstart 60s rise 2 fall 5 weight 10
    server  172.16.2.22:8003  172.16.2.22:8003 id 04 cookie img_old04 check inter 2s downinter 5s slowstart 60s rise 2 fall 5 weight 10

