#却除nginx里含有200状态码的日志filter配置
if ($programname == 'webFront-nginx-z-access' or $programname == 'webFront-nginx-www-access' or $programname == 'webFront-nginx-z-error' or $programname == 'webFront-nginx-www-error' or $programname == 'webFront-nginx-jz-access' or $programname == 'webFront-nginx-shouji-access' or $programname == 'webFront-nginx-zuoye-access' or $programname == 'webFront-nginx-1dui1-access' or $programname == 'webFront-nginx-m-access' or $programname == 'webFront-nginx-analytics-access' or $programname == 'webFront-nginx-ctb-qingguo-access' or $programname == 'webFront-nginx-cyfls-qingguo-access' or $programname == 'webFront-nginx-www-qingguo-access') and (($msg contains '  rc:200  ') or ( $msg contains '  rc:404  ')  or ($msg contains '  rc:301  ') or ($msg contains '  rc:302  ') or ( $msg contains '  rc:304  ') or ($msg contains 'fcgi_cache'))  then stop
#过滤jic的No such file or directory关键字日志
if ( ( $programname == 'webFront-nginx-jic-error'  or $programname == 'webFront-nginx-z-error' or $appName == 'webFront-nginx-www-error') and $msg contains 'No such file or directory' ) then stop

#syslog 转发到relay机器进行邮件与短信提醒
$ActionQueueFileName acQweb
$ActionQueueMaxDiskSpace 200G
$ActionQueueSaveOnShutdown on
$ActionQueueMaxFileSize 512M
$ActionQueueType LinkedList
$ActionResumeRetryCount -1
#$ActionQueueDiscardMark 1500000
#$ActionQueueQueueDiscardSeverity 3
$ActionQueueHighWatermark 3000
$ActionQueueLowWatermark 1000
$ActionQueueTimeoutEnqueue 200
$ActionDequeueBatchSize 200

